        "@rollup/plugin-typescript": "^8.2.1",
        "@tsconfig/svelte": "^2.0.1",
        "@types/node": "^14.14.37",
        "babel-loader": "^8.2.2",
        "copy-webpack-plugin": "^9.0.1",
        "css-loader": "^5.2.6",
        "dotenv": "^10.0.0",
        "esbuild": "^0.14.2",
        "esbuild-svelte": "^0.6.0",
        "obsidian": "^0.12.11",
        "rollup": "^2.32.1",
